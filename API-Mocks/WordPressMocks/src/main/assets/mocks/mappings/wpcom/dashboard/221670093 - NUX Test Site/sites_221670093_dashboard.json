{
  "request": {
    "method": "GET",
    "urlPath": "/wpcom/v2/sites/221670093/dashboard/cards-data/",
    "queryParameters": {
      "_locale": {
          "matches": "(.*)"
      },
      "cards": {
          "equalTo": "todays_stats,posts,pages,activity"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "todays_stats": {
        "views": 0,
        "visitors": 0,
        "likes": 0,
        "comments": 0
      },
      "posts": {
        "has_published": false,
        "draft": [],
        "scheduled": []
      },
      "pages": [],
      "activity": {
        "@context": "https://www.w3.org/ns/activitystreams",
        "summary": "Activity log",
        "type": "OrderedCollection",
        "totalItems": 0,
        "page": 1,
        "totalPages": 0,
        "itemsPerPage": 5,
        "id": "https://public-api.wordpress.com/wpcom/v2/sites/221670093/activity"
      }
    }
  }
}
