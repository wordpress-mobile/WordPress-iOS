{
    "request": {
        "method": "GET",
        "urlPattern": "/wpcom/v2/sites/.*/rewind/capabilities.*",
        "queryParameters": {
            "locale": {
                "matches": "(.*)"
            }
        }
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "capabilities": []
        },
        "headers": {
            "Content-Type": "application/json",
            "Connection": "keep-alive",
            "Cache-Control": "no-cache, must-revalidate, max-age=0"
        }
    }
}
