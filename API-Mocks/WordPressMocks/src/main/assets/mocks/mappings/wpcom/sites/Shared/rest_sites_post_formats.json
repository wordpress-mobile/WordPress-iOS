{
  "request": {
    "method": "GET",
    "urlPattern": "/rest/v1.1/sites/.*/post-formats.*",
    "queryParameters": {
      "locale": {
        "matches": "(.*)"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "formats": {
        "aside": "Aside",
        "image": "Image",
        "video": "Video",
        "quote": "Quote",
        "link": "Link",
        "gallery": "Gallery",
        "status": "Status",
        "audio": "Audio",
        "chat": "Chat"
      }
    }
  }
}