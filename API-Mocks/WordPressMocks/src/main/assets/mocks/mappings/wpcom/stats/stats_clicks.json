{
  "request": {
    "method": "GET",
    "urlPath": "/rest/v1.1/sites/106707880/stats/clicks/",
    "queryParameters": {
      "period": {
        "equalTo": "day"
      },
      "max": {
        "equalTo": "[0-9]+"
      },
      "date": {
        "equalTo": "2019-07-17"
      },
      "locale": {
        "matches": "(.*)"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "date": "{{now format='yyyy-MM-dd'}}",
      "days": {
        "{{now format='yyyy-MM-dd'}}": {
          "clicks": [

          ],
          "other_clicks": 0,
          "total_clicks": 0
        }
      },
      "period": "day"
    }
  }
}