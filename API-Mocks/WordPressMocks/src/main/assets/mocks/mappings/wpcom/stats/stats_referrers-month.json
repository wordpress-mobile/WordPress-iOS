    "request": {
        "method": "GET",
        "urlPathPattern": "/rest/v1.1/sites/([0-9]+)/stats/referrers/",
        "queryParameters": {
            "period": {
                "equalTo": "month"
            },
            "max": {
                "matches": "[0-9]+"
            },
            "date": {
                "matches": "(.*)"
            },
            "locale": {
                "matches": "(.*)"
            }
        }
    },
    "response": {
        "status": 200,
        "jsonBody": {
