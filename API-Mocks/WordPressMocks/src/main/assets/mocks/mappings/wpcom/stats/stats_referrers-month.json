              "group": "Search Engines",
              "name": "Search Engines",
              "icon": "https://wordpress.com/i/stats/search-engine.png",
              "total": 1,
              "follow_data": null,
              "results": [
                {
                  "name": "Google Search",
                  "url": "http://www.google.com/",
                  "icon": "https://secure.gravatar.com/blavatar/6741a05f4bc6e5b65f504c4f3df388a1?s=48",
