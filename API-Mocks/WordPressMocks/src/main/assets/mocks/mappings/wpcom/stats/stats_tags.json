{
  "request": {
    "method": "GET",
    "urlPattern": "/rest/v1.1/sites/.*/stats/tags/.*",
    "queryParameters": {
      "locale": {
        "matches": "(.*)"
      },
      "max": {
        "matches": "(.*)"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "date": "{{now format='yyyy-MM-dd'}}",
      "tags": [

      ]
    },
    "headers": {
      "Content-Type": "application/json",
      "Connection": "keep-alive",
      "Cache-Control": "no-cache, must-revalidate, max-age=0"
    }
  }
}