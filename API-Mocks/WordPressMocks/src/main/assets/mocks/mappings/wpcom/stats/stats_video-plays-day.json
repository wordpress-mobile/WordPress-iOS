{
  "request": {
    "method": "GET",
    "urlPath": "/rest/v1.1/sites/106707880/stats/video-plays/",
    "queryParameters": {
      "period": {
        "equalTo": "day"
      },
      "max": {
        "matches": "[0-9]+"
      },
      "date": {
        "matches": "(.*)"
      },
      "locale": {
        "matches": "(.*)"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "date": "{{now format='yyyy-MM-dd'}}",
      "period": "day",
      "days": {
        "{{now format='yyyy-MM-dd'}}": {
          "plays": [

          ],
          "other_plays": 0,
          "total_plays": 0
        }
      }
    }
  }
}