  "request": {
    "method": "GET",
    "urlPathPattern": "/rest/v1.1/sites/([0-9]+)/stats/visits/",
    "queryParameters": {
      "unit": {
        "equalTo": "month"
      },
      "quantity": {
        "matches": "[0-9]+"
      },
      "date": {
        "matches": "(.*)"
      },
      "locale": {
        "matches": "(.*)"
      }
    }
  },
